{
    "name": "open-weather-app",
    "version": "1.0.1",
    "description": "Another front end UI for the open weather api",
    "main": "index.js",
    "scripts": {
        "test": "cross-env NODE_ENV=test jest --config=jest-config.js",
        "test-automation": "cross-env NODE_ENV=test jest --config=jest-config-puppeteer.js",
        "dev": "node server.js",
        "build": "next build",
        "clean": "next clean",
        "start": "NODE_ENV=production node server.js",
        "docker:build": "docker build -t frontend .",
        "docker:clean": "docker rm -f frontend || true",
        "docker:run": "docker run -p 3000:3000 --name frontend frontend",
        "docker:stop": "docker stop frontend",
        "docker:start": "docker start frontend && yarn run docker:logs",
        "docker:logs": "docker logs -f frontend",
        "deploy": "yarn run docker:build && yarn run docker:clean && yarn run docker:run"
    },
    "engines": {
        "node": "8.x"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.18.0",
        "compression": "^1.7.2",
        "create-react-class": "^15.6.3",
        "es6-promise": "^4.2.4",
        "express": "^4.16.2",
        "fontfaceobserver": "^2.0.13",
        "isomorphic-unfetch": "^2.0.0",
        "material-ui": "^0.20.0",
        "memory-cache": "^0.2.0",
        "moment": "^2.22.1",
        "next": "latest",
        "next-redux-saga": "^2.0.1",
        "next-redux-wrapper": "^1.3.5",
        "node-uuid": "^1.4.8",
        "prop-types": "^15.6.1",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-redux": "^5.0.7",
        "react-swipeable-views": "^0.12.13",
        "recompose": "^0.26.0",
        "redux": "^3.7.2",
        "redux-saga": "^0.16.0",
        "sessionstorage": "0.0.2",
        "validator": "^9.4.1"
    },
    "devDependencies": {
        "autoprefixer": "7.1.5",
        "cross-env": "^5.1.4",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "expect.js": "^0.3.1",
        "express": "^4.16.2",
        "glob": "^7.1.2",
        "jest": "^22.4.3",
        "jest-puppeteer": "^2.4.0",
        "node-sass": "^4.4.0",
        "normalize.css": "^7.0.0",
        "postcss-easy-import": "^3.0.0",
        "postcss-loader": "^2.0.7",
        "puppeteer": "^1.3.0",
        "raw-loader": "^0.5.1",
        "react-addons-test-utils": "^15.6.2",
        "react-test-renderer": "^16.3.2",
        "redux-devtools-extension": "^2.13.2",
        "redux-saga-testing": "^1.0.5",
        "regenerator-runtime": "^0.11.1",
        "sass-loader": "^6.0.6",
        "sinon": "^4.5.0",
        "supertest": "^3.0.0",
        "webpack": "^3.10.0"
    }
}
